{
    "compilerOptions": {
        "outDir": "bin",
        "target": "es2016",
        "module": "commonjs",
        "moduleResolution": "node",
        "declaration": true,
        "sourceMap": true,
        "stripInternal": true,
        "experimentalDecorators": true,
        "noFallthroughCasesInSwitch": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true
    },
    "files": [
        "assistant.ts",
        "config/index.ts",
        "config/vars.ts",
        "file.ts",
        "finetuningJob.ts",
        "getFile.ts",
        "getFiles.ts",
        "getFinetuningJob.ts",
        "getFinetuningJobs.ts",
        "getModel.ts",
        "getModels.ts",
        "index.ts",
        "provider.ts",
        "types/index.ts",
        "types/input.ts",
        "types/output.ts",
        "utilities.ts"
    ]
}
